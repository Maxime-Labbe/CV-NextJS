{
    "builds": [
      {
        "src": "app/api/python/route.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["scripts/**"]
        }
      },
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/app/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }