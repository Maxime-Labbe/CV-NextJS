{
   "builds": [
    {
      "src": "app/api/python/route.ts",
      "use": "@vercel/python",
      "config": {
        "includeFiles": ["scripts/**"]
      }
    },
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/app/api/$1"
      },
      {
        "src": "/app/(.*)",
        "dest": "/$1"
      }
    ]
  }